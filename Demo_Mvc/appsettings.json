{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "urls": "[http://your.url.com:80]",
  "ConnectionStrings": {
    "MySQL": "server=[ip_addr];port=3306;stmt=;user=[username];password=[password];database=[database];Charset=utf8;allowuservariables=true;"
  },
  "RedisConnectionString": {
    "Connection": "[ip_addr]:6379,abortConnect=false",
    "InstanceName": "Demo_Mvc_"
  },
  "JwtConfig": {
    "Issuer": "[http://your.url.com:80]",
    "Audience": "[http://your.url.com:80]",
    "SecretKey": "[---------------SecreKey--------------]"
  },
  "AppSettings": {
    "PwdSalt": "[PasswordSalt]",
    "Domain": "[https://your.url.com]"
  }
}
